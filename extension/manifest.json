{
    "manifest_version": 2,
    "name": "H5EQ",
    "version": "0.0.1",
    "description": "HTML5 equalization",
    "icons": {
        "48": "icons/48.png"
    },
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_icon": "icons/48.png",
        "default_title": "H5EQ",
        "default_popup": "popup/popup.html",
        "browser_style": false
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "lib/webextension-polyfill/browser-polyfill.js",
                "content_scripts/h5eq.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "lib/webextension-polyfill/browser-polyfill.js",
            "background_scripts/background.js"
        ]
    }
}